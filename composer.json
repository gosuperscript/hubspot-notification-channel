{
    "name": "digitalrisks/hubspot-notification-channel",
    "description": "Hubspot Notification Channel for laravel.",
    "keywords": [
        "digitalrisks",
        "laravel",
        "notifications",
        "hubspot"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Kani Robinson",
            "email": "kani.robinson@digitalrisks.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3|^8.0",
        "hubspot/hubspot-php": "^3.1",
        "illuminate/notifications": "5.7.*|5.8.*|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.5.7"
    },
    "autoload": {
        "psr-4": {
            "DigitalRisks\\Notifications\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DigitalRisks\\Tests\\Notifications\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "DigitalRisks\\Notifications\\ServiceProvider"
            ]
        }
    }
}
